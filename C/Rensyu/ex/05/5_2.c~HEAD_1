#include <stdio.h>

int main(void)
{
  int num1, num2;

  printf("整数を入力してください。\n");
  scanf("%d", &num1);
  scanf("%d", &num2);

  if (num1 < num2) {
    printf("%dより%dの方が多きい。\n",num1, num2 );
  }

  else if (num1 > num2){
    printf("%dより%dの方が大きい\n",num2,num1 );
  }

  else{
    printf("２つの値は同じです。\n");

  }
   return 0;
}
