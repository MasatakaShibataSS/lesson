#include <stdio.h>
#define NUM 5

int main(void)
{
int test[NUM];
int con;
int i, j;

con = 0;

printf("五人点数を入力してください。m(_ _)m\n");
for (i = 0; i < NUM; i++) {
  scanf("%d", &test[i]);

  if(75 <= test[i])
    con++;
}
for (j = 0; j < NUM; j++) {
  printf("%d番目の人の点数は%dです。\n",j+1,test[j]);
}
  printf("75点以上は%dです。\n", con);

   return 0;
}
